<h1>celebrities</h1>

{{#each theList}}


    <a href="/celebrities/{{this._id}}">
    <h3>Name: {{this.name}}</h3>
    </a>
    

    <h6>Occupation:  {{this.occupation}} </h6>

   
   <p>
       Catchphrase: {{this.catchphrase}}
   </p>

   <form action="/celebrities/delete/{{this._id}}" method="POST">
       <button>Delete This Celebrity</button>
   </form>

    




{{/each}}


<br>